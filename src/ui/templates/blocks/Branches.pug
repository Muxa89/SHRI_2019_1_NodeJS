.Branches
  .Branches-Header
    .Branches-Name Name
    .Branches-Hash Commit hash
  -for (let i = 0; i < branches.length; i++)
    .Branches-Row
      .Branches-Name
        span.Branches-Icon
        span.Branches-Text= branches[i].name
      .Branches-Hash
        span.Branches-Text.Link= branches[i].hash